@startuml
skinparam classAttributeIconSize 0

class User {
  +_id: ObjectId
  +name: String
  +email: String
  +password: String
  +role: String
  +matchPassword()
  +getSignedJwtToken()
}

class Product {
  +_id: ObjectId
  +name: String
  +address: String
  +tel: String
}

class Booking {
  +_id: ObjectId
  +bookingDate: Date
  +user: ObjectId
  +product: ObjectId
}

User "1" --> "0..*" Booking : makes
Product "1" --> "0..*" Booking : has

@enduml